<% provide(:title, 'Create Driver') %>
<h1>Create Driver</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@driver) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
    
      <%= f.label :license_id %>
      <%= f.text_field :license_id, class: 'form-control' %>
            
      <%= f.submit "Create Driver", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>